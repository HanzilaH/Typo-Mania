<AnchorPane fx:id="root" prefHeight="800.0" prefWidth="1000.0" style="-fx-background-color: #967E76;" xmlns="http://javafx.com/javafx/19" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.typo.MainPage">
